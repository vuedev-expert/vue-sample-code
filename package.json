{
  "name": "project-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "build:demo": "vue-tsc --noEmit && vite build --mode demo",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "preview": "vite preview",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore src",
    "lint:style": "stylelint src/**/*.{css,scss,vue}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.12",
    "@stripe/stripe-js": "^1.48.0",
    "@types/aws-iot-device-sdk": "^2.2.4",
    "@vueform/slider": "^2.1.7",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "@vuepic/vue-datepicker": "^3.6.8",
    "aws-iot-device-sdk": "^2.2.12",
    "aws-iot-device-sdk-v2": "^1.9.5",
    "aws-sdk": "^2.1331.0",
    "axios": "^0.27.2",
    "chart.js": "^4.2.1",
    "d3": "^7.8.2",
    "floating-vue": "^2.0.0-beta.20",
    "highcharts": "^10.3.3",
    "libphonenumber-js": "^1.10.21",
    "maska": "^1.5.2",
    "moment": "^2.29.4",
    "pinia": "^2.0.33",
    "qrcode": "^1.5.1",
    "v-tooltip": "^2.1.3",
    "vue": "^3.2.47",
    "vue-chartjs": "^5.2.0",
    "vue-color-kit": "^1.0.5",
    "vue-loading-overlay": "^5.0.3",
    "vue-multiselect": "^3.0.0-beta.1",
    "vue-router": "^4.1.6",
    "vue-select": "github:itlpps/vue-select#event-cleared",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-highcharts": "^0.1.3",
    "vue3-table-lite": "^1.3.7",
    "vue3-tel-input": "^1.0.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/d3": "^7.4.0",
    "@types/node": "^17.0.45",
    "@types/qrcode": "^1.5.0",
    "@types/vue-select": "^3.16.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^2.9.15",
    "vue-tsc": "^0.34.7"
  }
}
